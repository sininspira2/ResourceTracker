{
  "name": "resource-tracker",
  "version": "4.1.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run db:generate-hashes && next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "db:generate": "drizzle-kit generate && npm run db:generate-hashes",
    "db:generate-hashes": "tsx scripts/generate-migration-hashes.ts",
    "db:push": "drizzle-kit push",
    "db:log-init": "tsx --env-file .env.local scripts/log-initial-migration.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:check": "tsx --env-file .env.local scripts/check-migrations.ts",
    "db:check:drop": "tsx --env-file .env.local scripts/check-migrations.ts --drop",
    "update-version": "node scripts/update-version.js",
    "populate-resources": "tsx --env-file .env.local scripts/populate-resources.ts",
    "populate-resources-dune": "tsx --env-file .env.local scripts/populate-resources-dune.ts",
    "populate-blueprints-dune": "tsx --env-file .env.local scripts/populate-blueprints-dune.ts",
    "add-resources-with-images": "tsx --env-file .env.local scripts/add-resources-with-images.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@libsql/client": "^0.17.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.1.6",
    "next": "15.5.10",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0"
  },
  "overrides": {
    "nodemailer": "^7.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.58.1",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.10",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.5.10",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
