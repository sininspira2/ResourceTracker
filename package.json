{
  "name": "resource-tracker",
  "version": "4.0.7",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "tsx scripts/generate-migration-hashes.ts && next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "db:generate": "drizzle-kit generate && tsx scripts/generate-migration-hashes.ts",
    "db:push": "drizzle-kit push",
    "db:log-init": "tsx scripts/log-initial-migration.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:fix-state": "tsx --env-file .env.local scripts/fix-db-state.ts",
    "update-version": "node scripts/update-version.js",
    "populate-resources": "tsx --env-file .env.local scripts/populate-resources.ts",
    "populate-resources-dune": "tsx --env-file .env.local scripts/populate-resources-dune.ts",
    "populate-blueprints-dune": "tsx --env-file .env.local scripts/populate-blueprints-dune.ts",
    "add-resources-with-images": "tsx --env-file .env.local scripts/add-resources-with-images.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "drizzle-orm": "^0.44.6",
    "lucide-react": "^0.545.0",
    "nanoid": "^5.1.6",
    "next": "^15.1.1",
    "next-auth": "^4.24.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.8",
    "@types/react": "^19.2.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
